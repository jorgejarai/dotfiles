include "%L"

# Spanish ñ
<Multi_key> <n> <n> : "ñ"
<Multi_key> <N> <N> : "Ñ"

# Lesser than and greater than
<Multi_key> <l> <t> : "<"
<Multi_key> <g> <t> : ">"

# A more visual space character
<Multi_key> <backslash> <space> : "␣"

# Word joiner (U+2060, like &nbsp; but with width 0)
<Multi_key> <w> <j> : "⁠"

# Non-breaking space
<Multi_key> <Multi_key> <space> <space> : " "

# Square root
<Multi_key> <s> <q> : "√"

# Multiplication dot
<Multi_key> <d> <o> <t> : "⋅"

# Infinity
<Multi_key> <i> <n> <f> : "∞"

# Angle symbol
<Multi_key> <a> <n> <g> : "∠"

# Generic currency
<Multi_key> <dollar> : "¤"

